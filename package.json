{
  "name": "health-agent-kit",
  "version": "1.0.0",
  "description": "TypeScript framework for healthcare reinforcement learning agents - simulating clinical workflows, triage, and care pathways",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "web": "vite dev",
    "web:build": "vite build",
    "api": "ts-node-dev --respawn --transpile-only src/api/simple-server.ts",
    "start": "node dist/index.js",
    "start:full": "concurrently \"npm run api\" \"npm run web\"",
    "clean": "rimraf dist"
  },
  "keywords": [
    "healthcare",
    "reinforcement-learning",
    "clinical-workflows",
    "triage",
    "precision-medicine",
    "agent-simulation",
    "typescript"
  ],
  "author": "Health Agent Kit Team",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.10.0",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "date-fns": "^2.30.0",
    "zod": "^3.22.4",
    "rxjs": "^7.8.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/jest": "^29.5.8",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "rimraf": "^5.0.5",
    "vite": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.1.1",
    "recharts": "^2.8.0",
    "lucide-react": "^0.294.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-switch": "^1.0.3",
    "framer-motion": "^10.16.5",
    "tailwindcss-animate": "^1.0.7",
    "tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 